page {
    background: #cecece
}
.page-order-details {
    background: #fff;
    margin: 0 30rpx 100rpx;
    padding: 30rpx;
    position: relative
}
.page-order-details:after {
    height: 74rpx;
    content: '';
    background: url(data:image/png;base64,) repeat-x 0 bottom;
    background-size: auto 100%;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0
}
.order-details-info .images {
    margin-left: 80rpx;
    border-radius: 6rpx;
    overflow: hidden
}
.order-details-info .desc, .order-details-info .title {
    display: block;
    line-height: 1.22
}
.order-details-info .title {
    font-size: 32rpx;
    font-weight: 700;
    padding: 15rpx 0
}
.order-details-info .desc {
    font-size: 28rpx
}
.order-details-oter {
    padding-top: 30rpx
}
.order-details-oter .desc-address .desc, .order-details-oter .desc-box .desc {
    line-height: 1.2;
    margin-left: 10rpx;
    display: block
}
.order-details-oter .desc-address .desc {
    font-size: 30rpx
}
.order-details-oter .desc-box {
    margin-top: 20rpx;
    margin-bottom: 10rpx
}
.order-details-oter .desc-box .desc {
    font-size: 28rpx;
    color: #999
}
.code-box {
    position: relative;
    padding: 20rpx 0;
}
.playend {
    width: 209rpx;
    height: 169rpx;
    position: absolute;
    top: 10rpx;
    right: 10rpx;
    z-index: 2
}
.code-box .desc, .code-box .title {
    display: block
}
.code-box .title {
    font-weight: 700;
    padding: 20rpx 0
}
.code-box .desc {
    font-size: 28rpx;
    line-height: 1.4
}
.code-box .code-images {
    width: 400rpx;
    height: 400rpx;
    border-radius: 6rpx;
    border: 1rpx solid #ddd;
    margin: 30rpx auto
}
.code-end .desc {
    color: #999;
    color: #00958e;
}
.order-details-content {
    border-top: 8rpx solid #ddd;
    margin-top: 30rpx
}
.order-details-footer {
    border-top: 1rpx solid #ddd;
    padding-top: 30rpx;
    padding-bottom: 80rpx
}
.order-details-footer .items .desc {
    color: #999;
    font-size: 28rpx;
    line-height: 1.5
}
.order-details-footer .items .label {
    width: 140rpx
}
.qrcode{
    padding: 20rpx 0;
    text-align: center;
}
.qrcode image{
    width: 400rpx;
}