.confirm-pay {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: rgba(204, 204, 204, .8);
    z-index: 999;
}
.cp-main {
    background: #fff;
    width: 80%;
    margin: 200rpx auto;
    text-align: center;
    padding: 115rpx 0;
    border-radius: 3px;
}
.cp-tip {
    padding-bottom: 40rpx;
}
.cp-main navigator,.cp-main .btn-pay {
    width: 220rpx;
    padding: 20rpx;
    font-size: 28rpx;
    margin: 0 auto;
    background: #00958e;
    color: #fff;
    border-radius: 3px;
}
.points-tip{
    font-size: 26rpx;
    margin-left: 10rpx;
}
.membercard-new {
    background: #80A7DA;
    border-radius: 6rpx;
    padding: 20rpx;
    position: relative;
    margin-bottom: 30rpx;
    overflow: hidden;
}
.membercard-section {
    background: #fff;
    padding: 30rpx;
}
.membercard-price {
    text-align: center;
    font-size: 56rpx;
    padding: 22rpx 0 40rpx;
    color: #000;
}
.membercard-info {
    font-size: 24rpx;
    text-align: center;
}
.membercard-aside {
    padding-top: 20rpx;
    font-size: 24rpx;
    color: #fff;
}
.active{
    border-bottom: 1px solid #dcdcdc;
}
.active-num text:first-child{
    margin-right: 12rpx;
    color: #999;
}
.active-num text.active-curr{
    color: red;
}
.dialog,.dialog-mask{
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 101;
}
.dialog-mask{
    background: rgba(0, 0, 0, .7);
}
.dialog-content {
    position: fixed;
    z-index: 102;
    background: #fff;
    bottom: 0;
    width: 100%;
    height: 80%;
    text-align: center;
    padding-top: 40rpx;
    box-sizing: border-box;
}
.dialog-content label {
    display: flex;
    justify-content: space-between;
    align-items: center;
