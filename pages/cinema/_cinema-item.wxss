.cinema-container{font-size:26rpx;padding:38rpx 24rpx 38rpx 0;margin-left:24rpx;color:#666;position:relative;line-height:1;overflow:hidden}.cinema-container .label{position:absolute;right:-52rpx;top:-24rpx;color:#fff;background-color:#FDB400;padding:40rpx 40rpx 2rpx;transform:rotate(45deg);font-size:20rpx}.cinema-container .cinema-name{font-size:32rpx;color:#000;display:inline-block;max-width:500rpx;margin-top:-10rpx;padding-top:10rpx}.activity-tag{margin-top:14rpx;font-size:22rpx;color:#999}.activity-tag::before{border-radius:2px;color:#fff;padding:2px;margin-right:10rpx;display:inline-block}.platform-activity-tag::before{content:'惠';background-color:#F90}.merchant-activity-tag::before{content:'促';background-color:#FF7979}.card-promotion-tag::before{content:'卡';background-color:#A97BE0}.coupon-promotion-tag::before{content:'券';background-color:#00C7B3}.star-activity-tag::before{content:'星';color:#F987AD}.cinema-container .price{display:inline-block;margin-left:20rpx;font-size:22rpx;color:#EF4238;line-height:30rpx}.cinema-container .price .num{font-size:34rpx}.cinema-container .address{margin-top:10rpx;padding-top:10rpx}.cinema-container .distance{position:absolute;top:94rpx;right:24rpx;color:#999}.cinema-container .recentTimes{margin-top:20rpx}.cinema-container .tag-list view{margin-top:20rpx;display:inline-block;padding:2px;border:1rpx solid #589DAF;border-radius:2px;margin-right:3px;font-size:22rpx;color:#589DAF}.cinema-container .tag-list view.featrue{color:#F90;border-color:#F90}.cinema-container .tag-list view.vip{color:#fff;background-color:#F90;border-color:#F90}