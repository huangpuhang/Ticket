@import "_cinema-item.wxss";
page {
    font-size: 24rpx
}
.cinema-navi {
    background: #fff
}
.movie-info-container {
    height: 300rpx;
    padding: 38rpx 26rpx
}
.movie-image {
    float: left;
    width: 220rpx;
    height: 300rpx;
    border: 1rpx solid #F5F5F5
}
.movie-info {
    margin-left: 240rpx;
    color: #666;
    line-height: 1.5
}
.movie-info .movie-name {
    font-size: 34rpx;
    color: #333;
    font-weight: 700
}
.movie-info .movie-score {
    margin-top: 10rpx;
    font-size: 34rpx;
    color: #FF9A00
}
.movie-info .movie-score image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 6rpx;
    position: relative;
    top: 2rpx
}
.movie-info .movie-wish {
    margin-top: 16rpx;
    margin-bottom: 20rpx;
    font-size: 30rpx;
    color: #FF9A00
}
.movie-info .movie-wish::after {
    content: '人想看';
    font-size: 20rpx
}
.movie-info .score-num {
    margin-bottom: 10rpx
}
.movie-info .movie-category .tag-3d {
    margin-left: 20rpx;
    display: inline-block;
    padding: 4rpx 8rpx;
    font-size: 16rpx;
    height: 16rpx;
    line-height: 16rpx;
    background: #7fb0cb;
    border-radius: 4rpx;
    color: #fff;
    border-style: solid;
    border-color: #7fb0cb;
    border-width: 2rpx;
    position: relative;
    top: -2rpx
}
.movie-info .movie-category .tag-imax {
    display: inline-block;
    padding: 4rpx 8rpx;
    font-size: 16rpx;
    height: 16rpx;
    line-height: 16rpx;
    background: #fff;
    color: #7fb0cb;
    border-top-right-radius: 4rpx;
    border-bottom-right-radius: 4rpx;
    border-style: solid;
    border-color: #7fb0cb;
    border-width: 2rpx;
    position: relative;
    left: -4rpx;
    top: -2rpx
}
.plist-container {
    margin-top: 8rpx;
    height: 136rpx
}
.plist-container .plist {
    white-space: nowrap;
    padding-top: 12rpx
}
.plist-container .plist .show {
    display: inline-block;
    background-color: #FAFAFA;
    border: 1rpx solid #D9D9D9;
    margin-right: 20rpx;
    width: 140rpx;
    height: 120rpx;
    text-align: center;
    border-radius: 6rpx;
    position: relative
}
.plist-container .plist .show .next-day {
    position: absolute;
    background: #A3CFD9;
    border-radius: 4rpx;
    color: #fff;
    font-size: 18rpx;
    width: 80rpx;
    height: 24rpx;
    line-height: 24rpx;
    left: 30rpx;
    top: -12rpx
}
.plist-container .plist .show .tm {
    font-size: 34rpx;
    color: #333;
    margin-top: 12rpx
}
.plist-container .plist .show .cat {
    font-size: 22rpx;
    color: #999;
    margin-top: 10rpx
}
.plist-container .plist .show .pr {
    font-size: 22rpx;
    color: #EF4238;
    margin-top: 8rpx
}
.error-page.no-cinema {
    position: static;
    z-index: 0;
    margin-top: 300rpx
}
.flexbox, .flexbox-column-c, .flexbox-hv-c {
    display: flex
}
.flexbox-column {
    flex-direction: column
}
.flexbox-column-c, .flexbox-hv-c {
    align-items: center
}
.flexbox-hv-c {
    justify-content: center
}
.flex-0 {
    flex-shrink: 0
}
.flex-1 {
    flex: 1
}
.flex-wrap {
    flex-wrap: wrap
}
.flex-nowrap {
    flex-wrap: nowrap
}
.img200280 {
    width: 200rpx;
    height: 280rpx
}
image {
    width: 100%;
    height: 100%
}
.grade, .grade-after, .price {
    color: #f90;
    font-size: 30rpx;
    font-weight: 700;
    display: flex;
    align-items: center
}
.grade, .grade-after:after, .grade:before {
    display: bock
}
.grade-after {
    align-items: flex-end
}
.grade:before {
    content: '评分';
    color: #9ea7b4;
    font-size: 26rpx;
    font-weight: 400;
    padding-right: 6rpx
}
.grade-after:after {
    content: '分';
    font-size: 22rpx;
    font-weight: 400;
    padding-left: 6rpx
}
.movieinfo {
    padding: 30rpx;
    position: relative;
    background: #999;
    overflow: hidden
}
.movieinfo .maskimage {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: inherit;
    filter: blur(20rpx);
    z-index: 2;
    opacity: .8
}
.movieinfo .mmovie-item {
    position: relative;
    z-index: 3;
    min-width: 0;
    margin-right: 10rpx;
}
.cinema-arrow{
    width: 20rpx;
    margin-right: 10rpx;
}
.movieinfo .img200280 {
    margin-right: 20rpx;
    flex: none;
}
.movieinfo .title {
    padding: 0 0 20rpx 0
}
.movieinfo .title .enname, .movieinfo .title .name {
    display: block;
    color: #fff
}
.movieinfo .title .name {
    font-size: 32rpx;
    font-weight: 700;
    line-height: 1.2;
    padding-bottom: 10rpx
}
.movieinfo .title .enname {
    font-size: 24rpx
}
.movieinfo .desc{
    min-width: 0;
}
.movieinfo .desc .info, .movieinfo .desc .times, .movieinfo .desc .type {
    font-size: 24rpx;
    display: block;
    padding: 10rpx 0 0;
    color: #fff;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.movieinfo .desc .clamp1 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden
}
.quick-select {
    height: 80rpx;
    background: #fff;
    padding: 0 30rpx;
    border-top: 0
}
.quick-select:before {
    display: none
}
.quick-select .item {
    font-size: 28rpx;
    position: relative;
    margin-right: 30rpx;
    padding-right: 30rpx
}
.quick-select .item:after {
    content: '';
    position: absolute;
    top: -4rpx;
    bottom: -4rpx;
    right: 0;
    width: 1px;
    background: #ddd;
    z-index: 1
}
.mod-icon-search {
    width: 36rpx;
    height: 36rpx;
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% auto
}
.fixedbox {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 10;
    bottom: 0;
    background: rgba(0, 0, 0, .5);
    padding-bottom: 100rpx
}
.fixedbox .mod-line-20 {
    background: #f5f5f6
}
.fixed-search {
    padding-bottom: 200rpx;
    height: 80%;
    overflow: hidden
}
.search-box {
    background: #fff;
    padding: 30rpx
}
.search-box input {
    height: 52rpx;
    border-radius: 52px;
    background: #f5f5f6;
    box-sizing: border-box;
    padding: 0 30rpx
}
.history {
    background: #fff;
    padding: 30rpx
}
.history .box-desc .desc {
    font-size: 26rpx;
    color: #a1a1a1
}
.history .select {
    display: flex;
    flex-wrap: wrap
}
.history .select .item {
    flex-shrink: 0;
    margin: 20rpx 20rpx 0 0;
    background: #f5f5f6;
    border-radius: 30px;
    padding: 20rpx 80rpx 20rpx 30rpx;
    position: relative
}
.history .select .item image {
    width: 14rpx;
    height: 14rpx;
    position: absolute;
    right: 30rpx;
    top: 50%;
    margin-top: -7rpx
}
.history .select .item .desc {
    font-size: 26rpx
}
.search-end {
    background: #fff;
    height: 202rpx
}
.search-end .item {
    height: 100rpx;
    position: relative;
    padding: 0 30rpx
}
.search-end .item:after {
    height: 1px;
    overflow: hidden;
    background: #f0f0f0;
    content: '';
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    transform: scaleY(.5)
}
.search-end .item .title {
    display: block;
    font-size: 26rpx
}
.search-end .item .noticon {
    visibility: hidden
}
.cinema-main{
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
}
.cinema-cnt{
    flex: 1;
}
.cinema-num {
    display: flex;
    align-items: flex-end;
    justify-content: flex-end;
    padding-bottom: 10rpx;
    margin-top: 35rpx;
    margin-bottom: 10rpx;
}
.cinema-num view {
    display: flex;
    align-items: flex-end;
}
.cinema-num view text {
    color: #ff6666;
}
.cinema-num text {
    font-size: 22rpx;
    line-height: 1;
}
.cinema-num text:first-child {
    font-size: 38rpx;
    position: relative;
    top: 4rpx;
}
.cinema-tips {
    color: white;
    background: #f29700;
    font-size: 20rpx;
    -webkit-border-radius: 100rpx;
    border-radius: 100rpx;
    display: inline-block;
    padding: 5rpx 10rpx;
    width: fit-content;
    width: -webkit-fit-content;
    width: -moz-fit-content;
    position: absolute;
    bottom: 40rpx;
    right: 24rpx;
}
.cinema-origin {
    color: #999;
    text-decoration: line-through;
    font-size: 22rpx;
    padding-bottom: 4rpx;
}
.cinema-origin text:nth-child(2) {
    font-size: 32rpx;
}
.cinema-sale {
    color: #ff6633;
}
.cinema-sale text:nth-child(2) {
    font-size: 34rpx;
    font-weight: bold;
}